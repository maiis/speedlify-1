---
layout: layout.njk
---
<table class="leaderboard">
	<thead data-sr-only>
		<tr>
			<th>Vertical</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
	{%- for vertical, settings in sites %}
    <tr>
      <td><a href="/{{ vertical }}/">{{ vertical }}</a></td>
      <td>{{ settings.description }}</td>
    </tr>
	{%- endfor %}
	</tbody>
</table>